<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Automile PHP SDK - API Documentation</title>
    <parser>
        <target>docs/parser</target>
        <encoding>utf8</encoding>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <transformations>
        <template name="zend" />
    </transformations>
    <files>
        <directory>lib</directory>
    </files>
    <logging>
        <level>warn</level>
        <paths>
            <default>docs/log/{DATE}.log</default>
            <errors>docs/log/{DATE}.errors.log</errors>
        </paths>
    </logging>
</phpdoc>